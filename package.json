{
  "name": "tkr-portfolio",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://tuckertucker.github.io/tkr-portfolio",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "docs": "jsdoc -c jsdoc.json",
    "dev:portfolio": "vite",
    "build:portfolio": "vite build",
    "deploy:portfolio": "npm run clean && npm run build:storybook && npm run build:portfolio && gh-pages -d dist",
    "dev:storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "deploy:storybook": "npm run build:storybook && ./deploy-storybook"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.487.0",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.21.0",
    "@storybook/addon-docs": "^9.1.6",
    "@storybook/addon-onboarding": "^9.1.6",
    "@storybook/addon-vitest": "^9.1.6",
    "@storybook/react-vite": "^9.1.6",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^9.1.6",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "playwright": "^1.51.1",
    "prop-types": "^15.8.1",
    "storybook": "^9.1.6",
    "tailwindcss": "^3.4.3",
    "vite": "^6.2.0",
    "vitest": "^3.1.1"
